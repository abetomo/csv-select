{
  "name": "@abetomo/csv-select",
  "version": "0.0.6",
  "description": "It is a Calro application that runs SQL queries against csv data.",
  "keywords": [
    "csv",
    "sql",
    "carlo",
    "sqlite"
  ],
  "scripts": {
    "dev": "node --max-old-space-size=4096 ./scripts/dev.js",
    "build": "node --max-old-space-size=4096 ./scripts/build.js",
    "launch": "node ./scripts/carlo.js",
    "lint": "eslint --ext .js,.vue src scripts",
    "pkg": "npm run build && pkg . --out-path build/Release",
    "prepare": "npm run build",
    "test": "npm run lint && npm run unit",
    "unit": "echo \"Info: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abetomo/csv-select.git"
  },
  "author": "abetomo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abetomo/csv-select/issues"
  },
  "homepage": "https://github.com/abetomo/csv-select#readme",
  "dependencies": {
    "carlo": "^0.9.46"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-typescript": "^7.10.4",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@vue/component-compiler-utils": "^3.1.2",
    "bulma": "^0.7.5",
    "csv-parse": "^4.9.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "fs-extra": "^7.0.1",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4",
    "pkg": "^4.4.8",
    "sass": "^1.26.5",
    "sass-loader": "^7.3.1",
    "sql.js": "^0.5.0",
    "typescript": "^3.9.7",
    "typescript-eslint-parser": "^22.0.0",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.4.3",
    "vue-template-compiler": "^2.6.12"
  },
  "bin": {
    "csv-select": "./scripts/carlo.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json",
    "scripts"
  ],
  "pkg": {
    "assets": [
      "dist/*"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
