{
  "name": "@abetomo/csv-select",
  "version": "2.0.2",
  "description": "It is a Electron application that runs SQL queries against csv data.",
  "keywords": [
    "csv",
    "sql",
    "electron",
    "sqlite"
  ],
  "author": "abetomo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abetomo/csv-select.git"
  },
  "bugs": {
    "url": "https://github.com/abetomo/csv-select/issues"
  },
  "homepage": "https://github.com/abetomo/csv-select#readme",
  "main": "main.js",
  "bin": {
    "csv-select": "./scripts/electron.js"
  },
  "files": [
    "dist",
    "main.js",
    "preload.js",
    "scripts/electron.js"
  ],
  "scripts": {
    "dev": "npm run sql-wasm && nuxt",
    "dev-electron": "npm run sql-wasm && NODE_ENV=dev electron main.js",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "prepublishOnly": "npm run sql-wasm && BASE_DIR=./ npm run generate",
    "test": "npm run lint && npm run unit",
    "unit": "echo \"Info: no jest test specified\" && exit 0",
    "sql-wasm": "cp node_modules/sql.js/dist/sql-wasm.wasm static/"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "electron": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.0",
    "core-js": "^3.22.4",
    "csv-parse": "^5.0.4",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "nuxt": "^2.15.8",
    "nuxt-buefy": "^0.4.22",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "sql.js": "^1.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "ts-jest": "^28.0.2",
    "vue-jest": "^3.0.7"
  }
}
